{
  "name": "endpoint-agent",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "npm run build.webcomponents && vue-cli-service serve --inline-vue",
    "build": "npm run build.webcomponents && vue-cli-service build --inline-vue",
    "lint": "vue-cli-service lint",
    "build.webcomponents": "npm run build.webcomponents.development && npm run build.webcomponents.production",
    "build.webcomponents.development": "vue-cli-service build --dest ./public/local --mode development --target wc --inline-vue ./src/components/EndpointAgentRoot.vue && rm ./public/local/demo.html",
    "build.webcomponents.production": "vue-cli-service build --dest ./public/export --mode production --target wc --inline-vue ./src/components/EndpointAgentRoot.vue && rm ./public/export/demo.html"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-eslint": "^3.2.0",
    "@vue/cli-service": "^4.2.3",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
